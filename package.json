  },
  "peerDependencies": {
    "@effect/platform": "^0.93.3",
    "effect": "^3.19.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@effect/language-service": "0.73.1",
    "@effect/platform": "^0.94.4",
    "@effect/platform-node": "^0.104.1",
    "@effect/vitest": "^0.27.0",
    "@types/node": "^25.2.3",
    "effect": "^3.19.16",
    "playwright": "^1.58.2",
    "tsdown": "0.20.3",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.17",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
